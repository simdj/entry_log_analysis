    s3=scatter3(ith_cluster(:,1),ith_cluster(:,2),ith_cluster(:,3),100);
    s3.Marker=shape_list(i);
    s3.MarkerEdgeColor=edge_color_list{i};
    s3.MarkerFaceColor=edge_color_list{i};
end


